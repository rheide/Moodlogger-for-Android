<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.travenect.moodlogger"
      android:versionCode="1"
      android:versionName="1.0">
    <uses-sdk android:minSdkVersion="10" />
    <uses-library android:name="com.google.android" />
	<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" ></uses-permission>
	<uses-permission android:name="android.permission.INTERNET" />
    <application android:name=".Moodlogger" android:icon="@drawable/icon" android:label="@string/app_name" android:debuggable="true">
        <activity android:name=".main.MoodLoggerActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".main.MoodActivity" />
        <activity android:name=".main.AccountActivity" />
        <activity android:name=".main.StatsActivity" />
        <activity android:name=".main.HistoryActivity" />
    </application>
</manifest>